    "minimum-stability": "stable",
    "license": "ISC",
    "authors": [
        {
            "name": "Leo Di Donato",
            "email": "leodidonato@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.2"
    },
    "autoload": {
        "psr-0": {
